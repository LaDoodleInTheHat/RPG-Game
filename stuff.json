{
    "liveServer.settings.CustomBrowser": "chrome",
    "liveServer.settings.donotShowInfoMsg": true,
    "explorer.confirmDelete": false,
    "files.autoSave": "afterDelay",
    "editor.accessibilitySupport": "off",
    "explorer.confirmDragAndDrop": false,
    "liveServer.settings.donotVerifyTags": true,
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.editor.empty.hint": "hidden",
    "workbench.preferredHighContrastColorTheme": "Default Dark Modern",
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "*.exe": "default",
        "*.VSIXPackage": "default",
        "*.zip": "default"
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": false,
        "scminput": false
    },
    "git.enableSmartCommit": true,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "git.confirmSync": false,
    "githubPullRequests.createOnPublishBranch": "never",
    "C_Cpp.default.compilerPath": "c:\\mingw64\\bin\\g++.exe",
    "git.ignoreMissingGitWarning": true,
    "editor.fontSize": 12.9, 
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorSurroundingLines": 5,
    "editor.cursorSurroundingLinesStyle": "all",
    "editor.cursorWidth": 2,
    "editor.fontFamily": "'JetBrains Mono'",
    "editor.fontLigatures": true,
    "editor.minimap.autohide": "mouseover",
    "workbench.colorTheme": "GitHub Dark Default",
    "editor.inlineSuggest.edits.showCollapsed": true,
    "errorLens.enabled": true,
    "errorLens.fontWeight": "500",
    "errorLens.fontSize": "11.5px",
    "errorLens.gutterIconSet": "defaultOutline",
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.minimap.enabled": false,
    "window.menuBarVisibility": "compact",
    "git.openRepositoryInParentFolders": "always",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.largeFileOptimizations": false
}